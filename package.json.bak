{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./build/index.js",
    "typecheck": "tsc -b"
	"generate:css": "tailwindcss -o ./app/styles/tailwind.css",
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "@remix-run/node": "^1.7.5",
    "@remix-run/react": "^1.7.5",
    "flowbite": "^1.5.3",
    "flowbite-react": "^0.3.4",
    "isbot": "^3.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.5",
    "@remix-run/eslint-config": "^1.7.5",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.13",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vite-tsconfig-paths": "^3.5.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">=14"
  }
}
